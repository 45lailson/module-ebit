<?xml version="1.0"?>

<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp/etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="ebit-script" type="host">*.ebit.com.br</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="ebitempresa-style" type="host">*.ebit.com.br</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="ebit-img" type="host">*.ebit.com.br</value>
                <value id="ebitempresa-img" type="host">*.ebitempresa.com.br</value>
                <value id="newimgebit-img" type="host">newimgebit-a.akamaihd.net</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
